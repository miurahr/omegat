{
  "problemMatcher": [
    {
      "owner": "spotbugs",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^.*(?:H|M|L)\\s+(?:[A-Z])\\s+([A-Z0-9_]+)\\s[A-Za-z0-9]+:\\s+(.*)\\s\\s(?:Dereferenced at|Method invoked at|At)\\s+([A-Za-z0-9_]+\\.java):\\[lines? (\\d+(?:-\\d+)?)\\]",
          "file": 3,
          "line": 4,
          "message": 2,
          "code": 1
        }
      ]
    },
    {
      "owner": "spotbugs-OBL",
      "pattern": [
        {
          "regexp": "^.*M X ([A-Z0-9_]+)\\s[A-Z0-9]+:\\s+((?:.*)\\s\\sAt\\s+([A-Za-z0-9_]+\\.java):\\[line (\\d+)\\] is not discharged)",
          "file": 3,
          "line": 4,
          "message": 2,
          "code": 1
        }
      ]
    },
    {
      "owner": "spotbugs-no-line-number",
      "pattern": [
        {
          "regexp": "^.*(?:H|M|L)\\s+(?:[A-Z])\\s+([A-Za-z0-9_]+)\\s+[A-Za-z0-9]+:\\s+(.*)\\s+In\\s+([A-Za-z0-9_]+\\.java)",
          "file": 3,
          "message": 2,
          "code": 1
        }
      ]
    },
    {
      "owner": "checkstyle",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^.*\\[(?:(ERROR)|WARN)\\]\\s+(.*):(\\d+)(:\\d+)?:\\s+(.*)\\s+\\[(.*)\\]$",
          "severity": 1,
          "file": 2,
          "line": 3,
          "column": 4,
          "message": 5,
          "code": 6
        }
      ]
    }
  ]
}
